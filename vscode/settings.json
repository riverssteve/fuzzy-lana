{
    "editor.fontFamily": "iA Writer Mono S, Input Mono",
    "editor.fontSize": 13,
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        72,
        100,
        120
    ],
    "editor.tabCompletion": "on",
    "files.autoSave": "afterDelay",
    "git.autofetch": true,
    "python.analysis.downloadChannel": "beta",
    "python.jediEnabled": false,
    "python.venvPath": "~/.local/share/virtualenvs",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
    "terminal.integrated.env.osx": {
        "PYTHONDONTWRITEBYTECODE": "true",
        "PYTHONUNBUFFERED": "true",
    },
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontFamily": "SF Mono, Input Mono",
    "terminal.integrated.lineHeight": 1.3,
    "terminal.integrated.shellArgs.osx": [],
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Horizon Italic",
    "workbench.iconTheme": "quill-icons",
    "workbench.colorCustomizations": {
        "contrastBorder": "#1F212A",
    }
}